{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "creativeorange/gravatar": "^1.0",
        "fideloper/proxy": "^4.0",
        "jwage/php-apns": "^0.0.3",
        "laracasts/flash": "^3.0",
        "laravel-notification-channels/apn": "dev-master",
        "laravel-notification-channels/webpush": "^3.0",
        "laravel/cashier": "^9.1",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "lexxyungcarter/ziggy": "0.6.9.3",
        "predis/predis": "^1.1",
        "rollbar/rollbar-laravel": "^4.0"
    },
    "require-dev": {
        "ahuggins/open-on-make": "^0.0.25",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^3.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
